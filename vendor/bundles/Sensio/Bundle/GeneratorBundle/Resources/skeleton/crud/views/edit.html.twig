<h1>{{ entity }} edit</h1>

<form action="{{ "{{ path('"~ route_prefix ~"_update', { 'id': entity.id }) }}" }}" method="post" method="post" {{ "{{ form_enctype(form) }}" }}>
    {{ "{{ form_widget(form) }}" }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

{% set hide_edit, hide_delete = true, false %}
